    "@emotion/styled": "^11",
    "@stitches/react": "^0.1.5",
    "@types/brainhubeu__react-carousel": "^2.0.2",
    "add": "^2.0.6",
    "axios": "^0.21.1",
    "bowser": "^2.11.0",
    "framer-motion": "^4",
    "jsonp": "^0.2.1",
    "mobx": "^6.2.0",
    "mobx-react-lite": "^3.2.0",
    "query-string": "^7.0.0",
    "react": "^17.0.0",
    "react-cookie-consent": "^6.2.4",
    "react-count-to": "^0.12.0",
    "react-countup": "^4.3.3",
    "react-dom": "^17.0.0",
