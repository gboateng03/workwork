{
  "name": "viteapp",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Starter pack using Vite, React, Typescript, Chakra and Cypress",
  "keywords": [
    "vite",
    "react",
    "typescript",
    "chakra"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "tsc && yarn dev",
    "test": "concurrently \"yarn start \" \"cypress open\"",
    "test:ci": "cypress run --headless",
    "gen:gql": "generate-graphql-client -p -e https://nsv3-hasura.iotex.io/v1/graphql -o ./src/generated/gql",
    "sitemap": "npx ts-node --project tsconfig.node.json ./sitemap-builder.ts"
  },
  "dependencies": {
    "@brainhubeu/react-carousel": "^2.0.4",
